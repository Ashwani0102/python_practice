# Program to keep asking user to enter the positive number until they enter negative number and add all those postive number  
count = 0
while True:
    num = int(input("Enter the num(negative number to stop) :"))
    if num>=1:
        count=count+num
    else:
        break
print("Count is :",count)    

# Find all the prime number between 1 to 100
list = []
i=2
while i<=100:
      is_prime=True
      div=2
      while div<i:
        if i%div==0:
           is_prime=False
           break 
        div=div+1 
      if is_prime:
          list.append(i)
      i=i+1   
print(list)       


# Armstrong number between 1 to 1000
arm =[]
count=0
num=1
while num<=1000:
      total=0
      num_dig=len(str(num))
      temp_num=num
      while temp_num>0:
          digit=temp_num%10
          total +=digit**num_dig
          temp_num=temp_num//10 
      if total==num: 
         count=count+1
         arm.append(num)  
      num=num+1   
print("Number of Armstrong number between 1 to 1000 is :-",count)
print(arm)

# Smallest Number divisible by all  1 to 10
n=10
while True:
    divible=True
    i=1
    while i<=10:
        if n%i != 0:
            divible=False
            break
        i=i+1
    if divible:
        break
    n=n+1
print(n) 

# Program to check if a number is a perfect number
num = int(input("Enter the number :"))
i=1
li=[]
count=0
while i<num:
    if num%i==0:
        count=count+i
        li.append(i)
    i=i+1
if count==num:
    print("Number is perfect number")
    print("Factors for that number is :",li) 
else:
    print("Number is not perfect number") 

