# No of factors
num = int(input("Enter the number: "))
count = 0
divisor = 1
while divisor<=num:
    if num%divisor==0:
       count = count+1
    divisor=divisor+1
print(count)

# Append the factors in list
num = int(input("Enter the number: "))
count = 0
divisor = 1
list=[]
while divisor<=num:
    if num%divisor==0:
       list.append(divisor)
       count = count+1
    divisor=divisor+1
print(count)
print("The factors are : ",list)

# Append even factor of a number in list
num = int(input("Enter the number: "))
count = 0
divisor = 1
list=[]
while divisor<=num:
    if num%divisor==0:
       if divisor%2==0:
        list.append(divisor)
       count = count+1
    divisor=divisor+1
print(count)
print("The factors are : ",list)

